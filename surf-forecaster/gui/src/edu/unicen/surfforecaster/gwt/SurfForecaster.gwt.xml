<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 1.7.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/1.7.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='surfforecaster'>
  <!-- Inherit the core Web Toolkit stuff.                        -->
  <inherits name='com.google.gwt.user.User'/>

  <!-- Inherit the default GWT style sheet.  You can change       -->
  <!-- the theme of your GWT application by uncommenting          -->
  <!-- any one of the following lines.                            -->
  <!-- <inherits name='com.google.gwt.user.theme.standard.Standard'/> -->
  <inherits name='com.google.gwt.user.theme.chrome.Chrome'/>
  <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->
  
  <!-- Custom stylesheet -->
  <stylesheet src="../SurfForecaster.css"/>

  <!-- I18N inherits                                      -->
  <inherits name="com.google.gwt.i18n.I18N"/>
  
  <inherits name='edu.unicen.surfforecaster.common.GWTShineCommon' />
  
  <!-- Google Visualizations                                      -->
  <inherits name='com.google.gwt.visualization.Visualization'/>
  
  <!-- Google Maps                                      -->
  <inherits name="com.google.gwt.maps.GoogleMaps" />
  <!-- If you want to deploy this application outside of localhost,
       you must obtain a Google Maps API key                      -->
  <script src="http://maps.google.com/maps?gwt=1&amp;file=api&amp;v=2&amp;sensor=false&amp;key=ABQIAAAAS3DdacZND-p5k9wiKIuVERRWEwDACGOMA3psBdPfW884vKkS7BTrCiKC6rdBb0Pc48l5ASN7U9EGpw" />

  <!-- Specify the app entry point class.                         -->
  <entry-point class='edu.unicen.surfforecaster.gwt.client.SurfForecaster'/>
  
  <!-- English Locale                                      -->
  <extend-property name="locale" values="en"/>
  <!-- Spanish Locale                                      -->
  <extend-property name="locale" values="es"/>
  
</module>
